{
  "name": "test_project",
  "version": "0.0.0",
  "description": "TypeScript + Sequelize + MySQL + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start:prod": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "start": "npm run dev",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "nyc jest --config jest.config.js --forceExit --detectOpenHandles",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "migration": "sequelize-cli db:migrate --config=src/config/config.json --migrations-path=src/migrations",
    "migration:undo": "sequelize-cli db:migrate:undo:all --config=src/config/config.json --migrations-path=src/migrations",
    "migration:undo:last": "sequelize-cli db:migrate:undo --config=src/config/config.json --migrations-path=src/migrations",
    "migration:skeleton": "npx sequelize-cli migration:generate --name migration-skeleton --config=src/config/config.json --migrations-path=src/migrations",
    "seeders:skeleton": "npx sequelize-cli seed:generate --name demo-user-seeders-skeleton --config=src/config/config.json --seeders-path=src/seeders",
    "seed": "npx sequelize-cli db:seed:all --config=src/config/config.json --seeders-path=src/seeders",
    "lint-staged": "lint-staged",
    "cron":"npx ts-node -r tsconfig-paths/register cronService/index.ts"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@snapshot-labs/snapshot.js": "^0.4.74",
    "@types/nodemailer": "^6.4.6",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.20.2",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "envalid": "^7.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "joi": "^17.6.1",
    "js-big-decimal": "^1.3.12",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "nyc": "^15.1.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^19.2.2",
    "sequelize": "^6.21.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "uninstall": "^0.0.0",
    "uuid": "^3.4.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
